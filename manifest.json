{
  "manifest_version": 3,
  "author": "Center for Threat-Informed Defense <ctid@mitre-engenuity.org>",
  "name": "ATT&CK Powered Suit",
  "description": "Quick access to ATT&CK resources.",
  "version": "0.2.0",
  "icons": {
    "16": "public/image/logo-16.png",
    "48": "public/image/logo-48.png",
    "128": "public/image/logo-128.png",
    "192": "public/image/logo-192.png"
  },
  "background": {
    "service_worker": "public/worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "public/content.js"
      ]
    }
  ],
  "omnibox": {
    "keyword": "attack"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus",
    "storage"
  ],
  "action": {
    "default_popup": "public/index.html",
    "default_icon": {
      "16": "public/image/logo-16.png",
      "32": "public/image/logo-32.png",
      "48": "public/image/logo-48.png",
      "128": "public/image/logo-128.png"
    }
  },
  "options_page": "public/index.html?view=settings"
}
